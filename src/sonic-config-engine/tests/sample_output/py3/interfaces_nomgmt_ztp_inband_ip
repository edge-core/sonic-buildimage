# The loopback network interface
auto lo
iface lo inet loopback
   address 127.0.0.1
   netmask 255.255.0.0
   scope host
   post-up ip addr del 127.0.0.1/8 dev lo

# The management network interface
auto eth0


# ZTP out-of-band interface
allow-hotplug eth0
iface eth0 inet dhcp
iface eth0 inet6 dhcp
    up sysctl net.ipv6.conf.eth0.accept_ra=1
    down sysctl net.ipv6.conf.eth0.accept_ra=0


# ZTP in-band interface Ethernet0
auto Ethernet0
allow-hotplug Ethernet0
iface Ethernet0 inet dhcp
iface Ethernet0 inet6 dhcp

#
source /etc/network/interfaces.d/*
#

