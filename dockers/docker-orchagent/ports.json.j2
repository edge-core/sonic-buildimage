[
{% set ports_with_speed_set=[] %}
{% if PORT %}
{% for port in PORT %}
{% if PORT[port].has_key('speed') %}
    {%- if ports_with_speed_set.append(port) -%}{%- endif -%}
{%- endif -%}
{% endfor %}
{% for port in ports_with_speed_set %}
    {
        "PORT_TABLE:{{ port }}": {
            "speed": "{{ PORT[port]['speed'] }}",
            "description": "{{ PORT[port]['description'] }}"
        },
        "OP": "SET"
    }{% if not loop.last %},{% endif %}

{% endfor %}
{% endif %}
]
